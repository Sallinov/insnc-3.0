{
	"functions": {
		"api/*.php": {
			"runtime": "vercel-php@0.7.4"
		}
	},
	"routes": [
		{
			"src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))$",
			"headers": {
				"Cache-Control": "public, max-age=31536000, immutable"
			},
			"dest": "/$1"
		},
		{
			"src": "/(.*)",
			"dest": "/api/index.php"
		}
	]
}