{
	"functions": {
		"api/*.php": {
			"runtime": "vercel-php@0.7.4"
		}
	},
	"routes": [
		{
			"src": "/(.*\\.(css|js|jpg|jpeg|png|gif|ico|svg|woff|woff2|ttf|eot|mp4))$",
			"headers": {
				"Cache-Control": "public, max-age=31536000, immutable"
			},
			"dest": "/$1"
		},
		{
			"src": "^/mobile/(.*)",
			"dest": "/mobile/$1"
		},
		{
			"src": "^/desktop/(.*)",
			"dest": "/desktop/$1"
		},
		{
			"src": "/(.*)",
			"dest": "/api/index.php"
		}
	]
}