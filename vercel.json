{
	"version": 2,
	"builds": [
		{
			"src": "**/*.php",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "."
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)\\.php$",
			"dest": "/$1.php",
			"headers": {
				"Content-Type": "text/html"
			}
		},
		{
			"src": "/(.*)",
			"dest": "/$1"
		}
	]
}